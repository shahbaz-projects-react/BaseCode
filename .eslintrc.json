{
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": ["react-app", "prettier"],
    "plugins": ["react", "prettier"],
    "parserOptions": {
      "ecmaVersion": 12
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "endOfLine":"auto",
          "printWidth": 80,
          "trailingComma": "es5",
          "semi": false,
          "doubleQuote":true,
          "jsxSingleQuote": true,
          "singleQuote": false,
          "useTabs": false,
          "tabWidth":4
        }
      ]
    }
  }